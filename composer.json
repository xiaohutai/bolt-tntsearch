{
    "name": "twokings/tntsearch",
    "description": "Full text search engine using ",
    "type": "bolt-extension",
    "keywords": [
        "fulltext",
        "fuzzy",
        "search"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\TwoKings\\TNTSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\TwoKings\\TNTSearch\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\TwoKings\\TNTSearch\\TNTSearchExtension",
        "bolt-icon": "bolt-icon.png"
    }
}
